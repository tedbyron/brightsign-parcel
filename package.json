{
  "name": "brs-parcel",
  "license": "MIT",
  "author": "Teddy Byron <ted@tedbyron.com>",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "browserslist": "chrome 60",
  "engines": {
    "node": ">= 10"
  },
  "targets": {
    "home": {
      "source": "src/home/index.html",
      "context": "browser",
      "includeNodeModules": {},
      "publicUrl": "."
    },
    "api": {
      "source": "api/index.ts",
      "context": "node",
      "includeNodeModules": {
        "@brightsign/networkstatus": false
      }
    }
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "scripts": {
    "build": "rimraf dist/*; parcel build --no-source-maps",
    "serve": "rimraf dist/home*; parcel serve --target home --open --lazy",
    "watch": "rimraf dist/*; parcel watch",
    "clean": "rimraf dist; rimraf .parcel-cache",
    "lint": "ts-standard --report stylish",
    "lint-fix": "ts-standard --report stylish --fix"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "express": "^4.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-retry": "^2.3.9"
  },
  "devDependencies": {
    "@parcel/optimizer-css": "^2.2.1",
    "@parcel/transformer-css-experimental": "^2.2.1",
    "@parcel/transformer-svg-react": "^2.2.1",
    "@parcel/validator-typescript": "^2.2.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.10",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "autoprefixer": "^10.4.2",
    "cssnano-preset-advanced": "^5.1.10",
    "parcel": "^2.2.1",
    "postcss": "^8.4.5",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.15",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.4"
  }
}
